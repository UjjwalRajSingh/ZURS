projection;
strict ( 2 );

define behavior for ZURS_TRAVEL_PROC alias Travel
{
  use create;
  use update;
  use delete;

  use association _Booking { create; }
}

define behavior for ZURS_BOOKING_PROC alias Booking
{
  use update;
  use delete;

  use association _Travel;
  use association _BookingSupplement { create; }
}

define behavior for ZURS_BOOKSUPPL_PROC alias BookingSuppl
{
  use update;
  use delete;

  use association _Travel;
  use association _Booking;
}